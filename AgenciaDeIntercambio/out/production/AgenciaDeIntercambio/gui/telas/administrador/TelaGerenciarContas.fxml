<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="502.0" prefWidth="667.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.controladores.administrador.TelaGerenciarContasController">
    <children>
        <VBox layoutX="184.0" layoutY="101.0" prefHeight="300.0" prefWidth="300.0">
            <children>
                <Button fx:id="buttonAdicionarConta" mnemonicParsing="false" onAction="#carregarTelaAdicionarConta" prefWidth="200.0" text="Adicionar uma conta">
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="buttonAtualizarConta" mnemonicParsing="false" onAction="#carregarTelaAtualizarConta" prefWidth="200.0" text="Atualizar uma conta">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttonRemoverConta" mnemonicParsing="false" onAction="#carregarTelaRemoverConta" prefWidth="200.0" text="Remover uma conta">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttonConsultarConta" mnemonicParsing="false" onAction="#carregarTelaConsultarConta" text="Consultar todas as contas">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="50.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
        <Button layoutX="303.0" layoutY="435.0" mnemonicParsing="false" onAction="#voltar" text="Voltar" />
    </children>
</AnchorPane>
