<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="502.0" prefWidth="667.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.controladores.administrador.TelaGerenciarCidadesController">
    <children>
        <VBox layoutX="184.0" layoutY="101.0" prefHeight="300.0" prefWidth="300.0">
            <children>
                <Button fx:id="buttonAdicionarCidade" mnemonicParsing="false" onAction="#carregarTelaAdicionarCidade" prefWidth="209.0" text="Adicionar uma cidade">
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="buttonAtualizarCidade" mnemonicParsing="false" onAction="#carregarTelaAtualizarCidade" prefWidth="209.0" text="Atualizar uma cidade">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttonRemoverCidade" mnemonicParsing="false" onAction="#carregarTelaRemoverCidade" prefWidth="209.0" text="Remover uma cidade">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="60.0" left="50.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttonConsultarCidade" mnemonicParsing="false" onAction="#carregarTelaConsultarCidade" text="Consultar todas as cidades">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="50.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
      <Button layoutX="303.0" layoutY="435.0" mnemonicParsing="false" onAction="#voltar" text="Voltar" />
    </children>
</AnchorPane>
